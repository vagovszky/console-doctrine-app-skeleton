{
    "name": "vagovszky/console-doctrine-app-skeleton",
    "description": "Skeleton for doctrine console app",
    "authors": [
        {
            "name": "Martin Vagovszky",
            "email": "vagovszky@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    
    "require": {
        "php": "^7.4",
        "doctrine/orm": "^2.7",
        "laminas/laminas-log": "^2.12@dev",
        "laminas/laminas-servicemanager": "^3.4@dev",
        "laminas/laminas-config": "^3.3@dev",
        "laminas/laminas-mail": "^2.11@dev",
        "symfony/console": "^5.2@dev",
        "symfony/event-dispatcher": "^5.1@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^9@dev"
    },
    "autoload": {
        "psr-4": {
            "Application\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ApplicationTest\\": "tests"
        }
    },
    "bin": ["bin/run.php"]
}
